{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "input_subsets": {
      "type": "object",
      "patternProperties": {
        "^[a-zA-Z0-9_-]+$": {"$ref": "#/definitions/subsetFields"}
      }
    },
    "output_subsets": {
      "type": "object",
      "patternProperties": {
        "^[a-zA-Z0-9_-]+$": {"$ref": "#/definitions/subsetFields"}
      }
    }
  },
  "required": ["input_subsets", "output_subsets"],
  "additionalProperties": false,
  "definitions": {
    "subsetFields": {
      "type": "object",
      "properties": {
        "fields": {
          "type": "object",
          "patternProperties": {
            "^[a-zA-Z0-9_-]+$": {
              "type": "object",
              "properties": {
                "type": {
                  "type": "string",
                  "enum": ["str", "int", "float", "bool", "bytes"]
                }
              },
              "required": ["type"],
              "additionalProperties": false
            }
          }
        }
      },
      "required": ["fields"],
      "additionalProperties": false
    }
  }
}